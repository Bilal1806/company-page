{
  "version": 2,
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/sign-up/?$", "dest": "/sign-up.html" },
    { "src": "^/login/?$", "dest": "/login.html" },
    { "src": "^/karir/?$", "dest": "/karir.html" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/libs/sweetalert/(.*)", "dest": "/libs/sweetalert/$1" }
  ]
}
